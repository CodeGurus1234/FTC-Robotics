-#  This file is app/views/teams/index.html.haml
%h1 All Teams

%table#teams
  %thead
    %tr
      %th Team  No
      %th Organization
      %th City
      %th State
      %th Date Registered
  %tbody
    - @teams.each do |team|
      %tr
        %td= team.team 
        %td= team.organization
        %td= team.city
        %td= team.state
        %td= team.date_registered.strftime('%m/%d/%Y')
        
= form_tag import_teams_path, multipart: true do

  = file_field_tag :file
  = submit_tag 'Upload CSV'
